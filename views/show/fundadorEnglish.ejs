<%- include("../partials/headerEnglish") %>
<section>
        <div class="row d-flex justify-content-center">
            <div class="col-md-3 d-none d-lg-block">
                <h4 class="text-center">Vinner mx</h4>
                <div class="list-group">
                    <!-- fundadores -->
                    <li class="list-group-item li-title">
                        Founders
                    </li>
                    <%fundadores.forEach(function(fundador){%>
                        <li class="list-group-item">
                            <a class="li-element" href="/equipo/fundadores/<%=fundador.show%>"><%=fundador.namesm%></a>
                        </li>
                    <%})%>
                        
                    <a href="/english/team" class="mt-3 btn btn-outline-info">Team</a>
                </div>
            </div>
            <div class="col-md-9 text-center">
                <h1>Meet the founders</h1>
                <div class="text-center show">
                    <h2><%= fundador.namelg %></h2> 
                    <img class="w-50" src="<%=fundador.img%>" class="card-img-top" alt="...">
                    <p class="my-3"><a class="btn btn-outline-info" target="tab" href="https://www.linkedin.com/in/valeria-toscano-alcala/">See LinkedIn</a></p>
                    <h5><%= fundador.description %></h5>
                    <div class="d-flex justify-content-center">
                        <p class="quote"><< <%=fundador.quote%> >></p>
                    </div>
                </div>
                <h1>#IAmVinner</h1>
            </div>
                  
        </div>
</section>

<%- include("../partials/footer") %>