<%- include("../partials/header") %>
<section>
        <div class="row d-flex justify-content-center">
            <div class="col-md-3 d-none d-lg-block">
                <h4 class="text-center">Vinner</h4>
                <div class="list-group">
                    <!-- fundadores -->
                    <li class="list-group-item li-title">
                        Consultants
                    </li>
                        <% consultores.forEach(function(consultor){%>
                            <li class="list-group-item">
                                <a class="li-element" href="/team/consultants/<%=consultor.show%>"><%=consultor.namesm%></a>
                            </li>  
                        <%})%>%>
                        
                    <a href="/english/team" class="mt-3 btn btn-outline-info">Equipo</a>
                </div>
            </div>
            <div class="col-md-9 text-center">
                <h1>Meet the consultants</h1>
                <div class="text-center show">
                    <h2><%= consultor.namelg %></h2> 
                    <img class="w-50" src="<%=consultor.img%>" class="card-img-top" alt="...">
                    <h5 class="mt-3"><%= consultor.description %></h5>
                </div>
                <h1>#IAmVinner</h1>
            </div>
                  
        </div>
</section>

<%- include("../partials/footer") %>