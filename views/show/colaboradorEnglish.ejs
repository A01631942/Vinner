<%- include("../partials/header") %>
<h1>test</h1>
<section>
        <div class="row d-flex justify-content-center">
            <div class="col-md-3 d-none d-lg-block">
                <h4 class="text-center">Vinner mx</h4>
                <div class="list-group">
                    <!-- fundadores -->
                    <li class="list-group-item li-title">
                        Associates
                    </li>
                    <%colaboradores.forEach(function(colaborador){%>
                        <li class="list-group-item">    
                            <a class="li-element" href="/team/associates/<%=colaborador.show%>"><%=colaborador.namesm%></a>
                        </li>
                    <%})%>
                        
                    <a href="/english/team" class="mt-3 btn btn-outline-info">Equipo</a>
                </div>
            </div>
            <div class="col-md-9 text-center">
                <h1>Meet the associates</h1>
                <div class="text-center show">
                    <h2><%= colaborador.namelg %></h2> 
                    <img class="w-50" src="<%=colaborador.img%>" class="card-img-top" alt="...">
                    <h5 class="mt-3"><%= colaborador.description %></h5>
                </div>
                <h1>#IAmVinner</h1>
            </div>
                  
        </div>
</section>

<%- include("../partials/footer") %>